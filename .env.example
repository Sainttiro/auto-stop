# Токен доступа к Tinkoff Invest API
# Получить можно в личном кабинете https://www.tinkoff.ru/invest/settings/api/
TINKOFF_TOKEN=your_tinkoff_token_here

# Настройки Telegram-бота для уведомлений
# Создать бота можно через @BotFather в Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
# ID чата для отправки уведомлений (можно получить через @userinfobot)
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
